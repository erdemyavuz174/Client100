   <scripts>
      <row>
         <process><![CDATA[!Normalize EXT values
:SET &IS_MISSING_EXT_VALUE# = STR_STARTS_WITH(&UC4RB_GIT_WORKING_DIR#, "EXT:")
:IF &IS_MISSING_EXT_VALUE# = "Y"
:  SET &UC4RB_GIT_WORKING_DIR# = ""
:ENDIF

:SET &IS_MISSING_EXT_VALUE# = STR_STARTS_WITH(&UC4RB_GIT_COMMIT#, "EXT:")
:IF &IS_MISSING_EXT_VALUE# = "Y"
:  SET &UC4RB_GIT_COMMIT# = ""
:ENDIF

:SET &IS_MISSING_EXT_VALUE# = STR_STARTS_WITH(&UC4RB_GIT_BRANCHNAME#, "EXT:")
:IF &IS_MISSING_EXT_VALUE# = "Y"
:  SET &UC4RB_GIT_BRANCHNAME# = ""
:ENDIF

:SET &IS_MISSING_EXT_VALUE# = STR_STARTS_WITH(&UC4RB_GIT_BRANCHNAME#, "origin/")
:IF &IS_MISSING_EXT_VALUE# = "N"
:  SET &UC4RB_GIT_BRANCHNAME# = "origin/&UC4RB_GIT_BRANCHNAME#"
:ENDIF

:SET &IS_MISSING_EXT_VALUE# = STR_STARTS_WITH(&UC4RB_GIT_LOCAL_REPO_NAME#, "EXT:")
:IF &IS_MISSING_EXT_VALUE# = "Y"
:  SET &UC4RB_GIT_LOCAL_REPO_NAME# = ""
:ENDIF

:SET &IS_MISSING_EXT_VALUE# = STR_STARTS_WITH(&UC4RB_GIT_INCLUDED_FILES#, "EXT:")
:IF &IS_MISSING_EXT_VALUE# = "Y"
:  SET &UC4RB_GIT_INCLUDED_FILES# = ""
:ENDIF

:SET &IS_MISSING_EXT_VALUE# = STR_STARTS_WITH(&UC4RB_GIT_EXCLUDED_FILES#, "EXT:")
:IF &IS_MISSING_EXT_VALUE# = "Y"
:  SET &UC4RB_GIT_EXCLUDED_FILES# = ""
:ENDIF

!Init git working dir
:IF &UC4RB_GIT_WORKING_DIR# = ""
:  PRINT "UC4RB_GIT_WORKING_DIR was not specified, use default location"
:  SET &CLIENT# = SYS_ACT_CLIENT()
:  SET &UC4RB_GIT_WORKING_DIR# = "../temp/ara/&CLIENT#/git_repos"
:ENDIF

!Init local repo name
:IF &UC4RB_GIT_LOCAL_REPO_NAME# = ""
:  SET &UC4RB_GIT_LOCAL_REPO_NAME# = &UC4RB_GIT_REMOTE_REPO_NAME#
:ENDIF

!Build git reset options base on given commit and branch name
:SET &UC4RB_GIT_RESET_OPTIONS# = " --hard"
:IF &UC4RB_GIT_COMMIT# <> ""
:  SET &UC4RB_GIT_RESET_OPTIONS# = "&UC4RB_GIT_RESET_OPTIONS# &UC4RB_GIT_COMMIT#"
:ELSE
:  SET &UC4RB_GIT_RESET_OPTIONS# = "&UC4RB_GIT_RESET_OPTIONS# &UC4RB_GIT_BRANCHNAME#"
:ENDIF]]></process>
      </row>
